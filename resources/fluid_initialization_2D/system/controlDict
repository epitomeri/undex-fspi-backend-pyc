FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "system";
  object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e-3;

writeControl    adjustableRunTime;

writeInterval   1e-5;

writeFormat     ascii;

writePrecision  12;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

deltaT          1e-8;

maxCo           0.5;

// ************************************************************************* //


functions
{
    overpressure
    {
        type overpressure;
        writeControl writeTime;
        store yes;
        pRef 101298;
    }
    probes
    {
        type blastProbes;
        probeLocations ((0.559 0 0));
        fields (p overpressure);
    }
}
