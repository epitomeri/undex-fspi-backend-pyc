FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

x       0.2;
nx     -0.2;
y       0.2;
ny     -0.2;
z       0.2;
nz     -0.2;

vertices
(
    ( $nx  $ny  $nz  )
    ( $x   $ny  $nz  )
    ( $x   $y   $nz  )
    ( $nx  $y   $nz  )

    ( $nx  $ny  $z   )
    ( $x   $ny  $z   )
    ( $x   $y   $z   )
    ( $nx  $y   $z   )

);

nx 40;
ny 40;
nz 40;

blocks
(
     hex (0 1 2 3 4 5 6 7) ($nx $ny $nz) simpleGrading (1 1 1)
);

boundary
(
//     ground
//     {
//         type wall;
//         faces
//         (
//             (0 1 2 3)
//         );
//     }
    outlet
    {
        type wall;
        faces
        (
            (0 1 2 3)

            (0 1 5 4)
            (2 3 7 6)
            (4 5 6 7)

            (1 2 6 5)
        );
    }
    symmx
    {
        type symmetryPlane;
        faces
        (
            (3 0 4 7)
        );
    }
);

mergePatchPairs
(
);

